# Base URL for the GitHub REST API requests made from the Next.js app.
NEXT_PUBLIC_API_BASE_URL=https://api.github.com

# Hostname/port used by the Next.js dev or production server.
HOST=127.0.0.1
PORT=3000
APP_HOST=127.0.0.1
APP_PORT=3000

# Override the URL Cypress uses when running integration tests.
CYPRESS_BASE_URL=http://127.0.0.1:3000

# Control the e2e helper script behavior.
# Use "dev" to run `next dev`, anything else will run `next start`.
E2E_SERVER_MODE=start
# Skip the Next.js production build step before running e2e tests.
E2E_SKIP_BUILD=false
